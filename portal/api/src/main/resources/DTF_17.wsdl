    <!-- Label Control -->
    <xsd:complexType name="LabelControl">
        <xsd:annotation>
            <xsd:documentation>Node for a label-control attribute representing a label in an InterviewScreen, EntityInstanceControl and ContainerControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:attribute name="style" type="xsd:string" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The label's style</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!-- Reference Relationship Control -->
    <xsd:complexType name="ReferenceRelationshipControl">
        <xsd:annotation>
            <xsd:documentation>Node for a reference-relationship-control attribute representing a reference relationship control in an InterviewScreen, EntityInstanceControl and ContainerControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:element name="error-list" type="TransactionErrorList" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="warning-list" type="TransactionWarningList" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="set-targets" type="RelationshipTargetList" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="possible-targets" type="RelationshipTargetList" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="properties" type="PropertiesList" minOccurs="0" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute name="relationship-id" type="xsd:string" use="required">
                    <xsd:annotation><xsd:documentation>The id of the relationship being collected</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="source-entity-id" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The id of the relationship's source entity</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="source-instance-id" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The id of the relationship's source entity instance</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="target-entity-id" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The id of the relationship's target entity</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="relationship-type" type="RelationshipCardinalityEnum" use="optional">
                    <xsd:annotation><xsd:documentation>The cardinality of this relationship. If the relationship is a OneToMany or ManyToMany then multiple targets can be set, otherwise only one relationship target can be set for this control at any given time</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="control-type" type="ControlTypeEnum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The input control type for this control. Can be either:
                            * checkbox - represents a control in which all possible targets of the relationship are rendered as a set of check boxes
                            * radio-button - represents a control in which all possible targets of the relationship are rendered as a set of radio buttons
                            * drop-down - represents a control in which all possible targets of the relationship are rendered as a set of options in a drop down control</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="RelationshipTargetList">
        <xsd:annotation>
            <xsd:documentation>Node for set-targets and possible-targets attributes in an ReferenceRelationshipControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="target" type="RelationshipTarget" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation><xsd:documentation>A single target entity instance</xsd:documentation></xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RelationshipTarget">
        <xsd:annotation>
            <xsd:documentation>Node for a target attribute in an RelationshipTargetList. The value of the node is the ID of a single target entity instance, while the attribute display-value is the string to represent the instance to the user.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="display-value" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The text to display for the target entity instance.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <!--Containment Relationship Control -->
    <xsd:complexType name="ContainmentRelationshipControl">
        <xsd:annotation>
            <xsd:documentation>Node for a containment-relationship-control attribute representing a control an InterviewScreen, EntityInstanceControl and ContainerControl. This control represents a control that lets you create instances of the target entity of a containment relationship. It is also known as an entity collect control.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:element name="error-list" type="TransactionErrorList" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="warning-list" type="TransactionWarningList" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="entity-instance-control" type="EntityInstanceControl" minOccurs="0"
                                 maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="relationship-id" type="xsd:string" use="required">
                    <xsd:annotation><xsd:documentation>The id of the relationship being collected</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="source-entity-id" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The id of the relationship's source entity</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="source-instance-id" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The id of the relationship's source entity instance</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="target-entity-id" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The id of the relationship's target entity</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="relationship-type" type="RelationshipCardinalityEnum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The cardinality of this relationship. In the case of a containment relationship it will either be:
                            * OneToOne - represents a one to one relationship
                            * OneToMany - represents a one to many relationship
                            The relationship cardinality is important as it determines the number of child instances that can be created. If the relationship is a OneToMany then multiple children can be set, otherwise only one child can be set for this control at any given time.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="display-style" type="EntityContainerDisplayTypeEnum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The display style of the control. Can be:
                            * portrait - generally means each child instance should be rendered vertically, in a business card fashion down the page
                            * tabular - generally means the control should be rendered as a table, with the controls representing the columns and the instances the rows</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="show-add-remove-controls" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Indicates the controls to add and remove instances should be rendered</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="add-instance-caption" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The caption for the add instance control</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="remove-instance-caption" type="xsd:string" use="optional">
                    <xsd:annotation><xsd:documentation>The caption for the remove instance control</xsd:documentation></xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!--Entity Group Control -->
    <xsd:complexType name="EntityGroupControl">
        <xsd:annotation>
            <xsd:documentation>Node for a entity-group-control attribute representing a control in an InterviewScreen, EntityInstanceControl and ContainerControl. This control represents a set of controls that are repeated for 0 or more instances of an entity.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:element name="error-list" type="TransactionErrorList" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="warning-list" type="TransactionWarningList" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="entity-instance-control" type="EntityInstanceControl" minOccurs="0"
                                 maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="entity-id" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The ID of the entity to show instances of</xsd:documentation>
