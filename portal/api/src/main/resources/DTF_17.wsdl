<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions name="odsInterviewService12.2.5_DTF_17"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"
                  xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
                  xmlns:typ="http://oracle.com/determinations/server/interview/12.2.5/rulebase/types"
                  xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/"
                  xmlns:i18n="http://www.w3.org/2005/09/ws-i18n"
                  targetNamespace="http://oracle.com/determinations/server/interview/12.2.5/rulebase/types">
  <wsdl:types>
            
<xsd:schema targetNamespace="http://xmlns.oracle.com/connector/webservice/12.2.1/data/types"
            xmlns="http://xmlns.oracle.com/connector/webservice/12.2.1/data/types"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            elementFormDefault="qualified"
            version="12.2.1.20150917">

    <!-- Data Types -->
    <xsd:complexType name="SeedDataDescription">
        <xsd:annotation>
            <xsd:documentation>Node for the tables attribute in the request of a Load data action, and for get-seed-data-definition-response in the response of the interview API's GetInputDataDefinition action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="table" type="Table" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A mapped Table.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LoadData">
        <xsd:annotation>
            <xsd:documentation>Node for the tables attribute in the response of a Load data action, and for the start-interview-request attribute in the request of the interview API's StartInterview action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="table" type="DataTable" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A table with instances (rows) populated by a web service connector.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UpdateData">
        <xsd:annotation>
            <xsd:documentation>Node for the tables attribute in the response of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="table" type="UpdateTable" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A table containing data retrieved by the web service connector.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SubmitData">
        <xsd:annotation>
            <xsd:documentation>Node for the tables attribute in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="table" type="SubmitTable" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A table with instances (rows) containing data collected from an interview.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Table Types -->
    <xsd:complexType name="Table">
        <xsd:annotation>
            <xsd:documentation>Node for the table attribute of SeedDataDescription in the request of a Load data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="field" type="Field" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A mapped Field.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="link" type="Link" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A mapped Link.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the Table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="DataTable">
        <xsd:annotation>
            <xsd:documentation>Node for a table attribute in LoadData in the response of a Load data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="row" type="DataRow" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>An instance of the table.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="UpdateTable">
        <xsd:annotation>
            <xsd:documentation>Node for a table attribute in UpdateData in the response of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="row" type="UpdateRow" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>An instance of the table.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="SubmitTable">
        <xsd:annotation>
            <xsd:documentation>Node for a table attribute in SubmitData in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="row" type="SubmitRow" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>An instance of the table.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of a table containing data to submit.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- Row Types -->
    <xsd:simpleType name="RowAction">
        <xsd:annotation>
            <xsd:documentation>Enumeration for a action attribute in SubmitRow in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="create">
                <xsd:annotation>
                    <xsd:documentation>The row has been created since most recent the last Save data action.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="update">
                <xsd:annotation>
                    <xsd:documentation>The row has been updated since the most recent Load or Save data action.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="delete">
                <xsd:annotation>
                    <xsd:documentation>The row has been deleted since the most recent Load or Save data action.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="DataRow">
        <xsd:annotation>
            <xsd:documentation>Node for a row attribute in DataTable in the response of a Load data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="field" type="DataField" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A field with a value populated by the web service connector.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="link" type="DataLink" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A link with values populated by the web service connector.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The ID which will be known internally for this row. If the row is involved in any subsequent Save actions, this id will be used to identify the row for the web service connector.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="UpdateRow">
        <xsd:annotation>
            <xsd:documentation>Node for a row attribute in UpdateTable in the response of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="DataRow">
                <xsd:attribute name="orig-id" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>If the id for the instance was changed from the original value supplied in the save-request, this attribute must hold the original value so that determination server knows which instance it is.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="SubmitRow">
        <xsd:annotation>
            <xsd:documentation>Node for a row attribute in SubmitTable in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="input-field" type="DataField" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A field with a value to submit to the connector.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="output-field" type="DataField" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A field to retrieve a value for in the save-response.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="link" type="DataLink" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A link from the row to specific rows in a target table.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="attachments" type="AttachmentList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A list of attachments to submit to the connector.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The ID of a row. If the row was pre-seeded from a Load request, the same id value will be used, otherwise it will be auto-generated by the determinations server.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="action" type="RowAction" use="required">
            <xsd:annotation>
                <xsd:documentation>The action the web service connector should take on the row.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- Field Types -->
    <xsd:complexType name="Field">
        <xsd:annotation>
            <xsd:documentation>Node for a field attribute in Table in the request of a Load or Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the mapped Field.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="field-type" type="AttributeTypeEnum" use="optional">
            <xsd:annotation>
                <xsd:documentation>The data-type of the mapped Field.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="DataField">
        <xsd:annotation>
            <xsd:documentation>Node for a field attribute in DataRow in the response of a Load, and for input-field and output-field in the SubmitRow in the request of a Save data action.
Note that a DataField only has one of the following fields as a child node.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Field">
                <xsd:sequence>
                    <xsd:choice minOccurs="0">
                        <xsd:element name="boolean-val" type="xsd:boolean">
                            <xsd:annotation>
                                <xsd:documentation>For boolean attributes, contains the value of the attribute</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="date-val" type="xsd:date">
                            <xsd:annotation>
                                <xsd:documentation>For date attributes, contains the value of the attribute</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="datetime-val" type="xsd:dateTime">
                            <xsd:annotation>
                                <xsd:documentation>For datetime attributes, contains the value of the attribute</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="time-val" type="xsd:time">
                            <xsd:annotation>
                                <xsd:documentation>For time attributes, contains the value of the attribute</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="number-val" type="xsd:decimal">
                            <xsd:annotation>
                                <xsd:documentation>For number or currency attributes, contains the value of the attribute</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="text-val" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>For text attributes, contains the value of the attribute</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="uncertain-val" type="UncertainValue">
                            <xsd:annotation>
                                <xsd:documentation>For attributes of any type, this field is used when the attribute has the value uncertain</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="unknown-val" type="UnknownValue">
                            <xsd:annotation>
                                <xsd:documentation>For attributes of any type, this field is used when the attribute has the value unknown</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:choice>
                    <xsd:element name="change-point" type="ChangePoint" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Temporal change points for the attribute value</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!-- Link Types -->
    <xsd:complexType name="Link">
        <xsd:annotation>
            <xsd:documentation>Node for a link attribute in Table in the request of a Load or Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the mapped Link.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="target" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the target Table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="DataLink">
        <xsd:annotation>
            <xsd:documentation>Node for a link attribute in DataRow in the response of a Load, and in SubmitRow in the response of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Link">
                <xsd:sequence>
                    <xsd:element name="ref" type="LinkRef" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>A reference to a row of the target table.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="LinkRef">
        <xsd:annotation>
            <xsd:documentation>Node for a ref attribute in DataLink in the request of a Load or Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The id of a row in the target table defined by the parent Link node's target attribute.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- Attachment Types -->
    <xsd:complexType name="AttachmentList">
        <xsd:annotation>
            <xsd:documentation>Node for the attachments attribute in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attachment" type="Attachment" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>An attachment, including any generated forms or items uploaded via an upload control.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Attachment">
        <xsd:annotation>
            <xsd:documentation>Node for the attachment attribute of AttachmentList in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Base64 encoded string of the attachment.</xsd:documentation>
                </xsd:annotation>
                <xsd:attribute name="name" type="xsd:string" use="required">
                <xsd:annotation>
                    <xsd:documentation>The name of the attachment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
                <xsd:attribute name="filename" type="xsd:string" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The filename of the attachment.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="description" type="xsd:string" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The description of the attachment. Forms have the description "Form auto generated by Intelligent Advisor.".</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <!-- Audit report types -->
    <xsd:complexType name="AuditReportList">
        <xsd:annotation>
            <xsd:documentation>Node for the audit-reports attribute in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="report" type="AttributeAuditNode" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The top level element for an audit report. There's a report for each instance of the entity containing the attribute selected for saving an audit report.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AttributeAuditNode">
        <xsd:annotation>
            <xsd:documentation>Node for the report attribute of AuditReportList, and for attribute-node of AttributeAuditNode in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="reference-tags" type="ReferenceTagList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Contains the reference tags for this audit report node.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="boolean-val" type="xsd:boolean">
                    <xsd:annotation>
                        <xsd:documentation>For boolean attributes, contains the value of the attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="date-val" type="xsd:date">
                    <xsd:annotation>
                        <xsd:documentation>For date attributes, contains the value of the attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="datetime-val" type="xsd:dateTime">
                    <xsd:annotation>
                        <xsd:documentation>For datetime attributes, contains the value of the attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="time-val" type="xsd:time">
                    <xsd:annotation>
                        <xsd:documentation>For time attributes, contains the value of the attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="number-val" type="xsd:decimal">
                    <xsd:annotation>
                        <xsd:documentation>For number or currency attributes, contains the value of the attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="text-val" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>For text attributes, contains the value of the attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="uncertain-val" type="UncertainValue">
                    <xsd:annotation>
                        <xsd:documentation>For attributes of any type, this field is used when the attribute has the value uncertain</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="unknown-val" type="UnknownValue">
                    <xsd:annotation>
                        <xsd:documentation>For attributes of any type, this field is used when the attribute has the value unknown</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="change-point" type="ChangePoint" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>When an attribute has temporal change points, these fields indicate where those change points occur</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="attribute-node" type="AttributeAuditNode">
                    <xsd:annotation>
                        <xsd:documentation>Decision report for another attribute that is relevant to the decision report for this attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="relationship-node" type="RelationshipAuditNode">
                    <xsd:annotation>
                        <xsd:documentation>Decision report for a relationship that is relevant to the decision report for this attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="already-proven-node" type="ReferenceAuditNode">
                    <xsd:annotation>
                        <xsd:documentation>A cross-reference to another decision report node that is relevant to the decision report for this attribute</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="node-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>An identifier for the decision report node. When an attribute occurs more than once in a decision report, a reference is made to the first appearance by using this identifier.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="attribute-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the attribute for this decision report node</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="attribute-type" type="AttributeTypeEnum" use="required">
            <xsd:annotation>
                <xsd:documentation>The data type of the attribute for this decision report node</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="entity-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the type of entity this attribute is contained in</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="instance-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The unique identifier of the entity instance for this attribute</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="is-user-set" type="xsd:boolean" use="required">
            <xsd:annotation>
                <xsd:documentation>The value of the attribute was set by the user</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="text" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The text of the decision report for this node. For example "The child's serves of vegetables per day is 3." or "The child does not live at home."</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="start-relevance" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation>If the relevance does not start at the beginning of time, the first date on which the attribute became relevant to the decision report. Can only be set if temporal rules or values are in use.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="end-relevance" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation>If the relevance does not continue to the end of time, the date on and after which the attribute was no longer relevant to the decision report. Can only be set if temporal rules or values are in use.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="RelationshipAuditNode">
        <xsd:annotation>
            <xsd:documentation>Node for the relationship-node attribute of AuditReportList and of RelationshipAuditNode in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="reference-tags" type="ReferenceTagList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Contains the reference tags for this audit report node.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="entity-node" type="EntityAuditNode">
                    <xsd:annotation>
                        <xsd:documentation>entity-node</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="attribute-node" type="AttributeAuditNode">
                    <xsd:annotation>
                        <xsd:documentation>Decision report for another attribute that is relevant to the decision report for this relationship</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="relationship-node" type="RelationshipAuditNode">
                    <xsd:annotation>
                        <xsd:documentation>Decision report for a relationship that is relevant to the decision report for this relationship</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="already-proven-node" type="ReferenceAuditNode">
                    <xsd:annotation>
                        <xsd:documentation>A cross-reference to another decision report node that is relevant to the decision report for this relationship</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="node-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>An identifier for the decision report node. When a relationship occurs more than once in a decision report, a reference is made to the first appearance by using this identifier.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="relationship-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the relationship for this decision report node</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="relationship-type" type="RelationshipCardinalityEnum" use="required">
            <xsd:annotation>
                <xsd:documentation>The cardinality of the relationship, as specified by the enumeration</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="source-entity-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the type of source entity to which this relationship refers</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="source-instance-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The unique identifier of the source entity instance for this relationship</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="target-entity-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the type of target entity to which this relationship refers</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="relationship-state" type="StateEnum" use="required">
            <xsd:annotation>
                <xsd:documentation>The relationship state, as specified by the enumeration</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="is-user-set" type="xsd:boolean" use="required">
            <xsd:annotation>
                <xsd:documentation>Whether the relationship is user-set, or inferred</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="text" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The text of the decision report for this node</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="start-relevance" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation>If the relevance does not start at the beginning of time, the first date on which the relationship became relevant to the decision report. Can only be set if temporal rules or values are in use.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="end-relevance" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation>If the relevance does not continue to the end of time, the date on and after which the relationship was no longer relevant to the decision report. Can only be set if temporal rules or values are in use.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="EntityAuditNode">
        <xsd:annotation>
            <xsd:documentation>Node for the entity-node element of RelationshipAuditNode in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="instance-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The unique identifier of the entity instance.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="text" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The text of the decision report for this node.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="ReferenceAuditNode">
        <xsd:annotation>
            <xsd:documentation>Node for the already-proven-node element of AttributeAuditNode and of RelationshipAuditNode in the request of a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="ref-node-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>A reference to another node, identified by its node-id, elsewhere in the decision report.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="ReferenceTagList">
        <xsd:annotation>
            <xsd:documentation>A container for reference tags</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="reference-tag" type="ReferenceTag" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The reference tag definition</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ReferenceTag">
        <xsd:annotation>
             <xsd:documentation>The reference tag</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:simpleType name="AttributeTypeEnum">
        <xsd:annotation>
            <xsd:documentation>The supported data types for attributes provided to and returned from an Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="boolean">
                <xsd:annotation>
                    <xsd:documentation>Attribute is type Boolean</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="text">
                <xsd:annotation>
                    <xsd:documentation>Attribute is type Text</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="number">
                <xsd:annotation>
                    <xsd:documentation>Attribute is type Number</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="currency">
                <xsd:annotation>
                    <xsd:documentation>Attribute is type Currency</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="date">
                <xsd:annotation>
                    <xsd:documentation>Attribute is type Date</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="datetime">
                <xsd:annotation>
                    <xsd:documentation>Attribute is type Datetime</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="timeofday">
                <xsd:annotation>
                    <xsd:documentation>Attribute is type Timeofday</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="StateEnum">
        <xsd:annotation>
            <xsd:documentation>The supported state types for relationships provided to and returned from an Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="known">
                <xsd:annotation>
                    <xsd:documentation>Relationship state is type known</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="unknown">
                <xsd:annotation>
                    <xsd:documentation>Relationship state is type unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="uncertain">
                <xsd:annotation>
                    <xsd:documentation>Relationship state is type uncertain</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="UnknownValue">
        <xsd:annotation>
            <xsd:documentation>Used to indicate the unknown value state for attribute and relationship values provided to and returned from an Assess action.
Tip: Any attributes, entities or relationships that are omitted from a request are also treated as unknown.</xsd:documentation>
        </xsd:annotation>
    </xsd:complexType>

    <xsd:complexType name="UncertainValue">
        <xsd:annotation>
            <xsd:documentation>Used to indicate the uncertain value for attribute values (and change points) provided to and returned from an Assess action. Unlike unknown, uncertain is considered a value, and prevents any further attempts to ask for or determine a value of that attribute.
Uncertain is most often used when a question has been asked of a real person, and the response is given that no answer is available. For example, if the question is "have you been vaccinated against measles" and the answer is "I don't know" you might use the uncertain value to indicate that the individual has neither definitely been vaccinated nor definitely not been vaccinated against measles.</xsd:documentation>
        </xsd:annotation>
    </xsd:complexType>

    <xsd:complexType name="ChangePoint">
        <xsd:annotation>
            <xsd:documentation>Temporal change points for attribute values and decision report nodes provided to and returned from an Assess action.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
            <xsd:element name="boolean-val" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>For boolean attributes, contains the value of the attribute. Must be set to either "true" or "false".</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="date-val" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation>For date attributes, contains the value of the attribute. Must be set to a valid date in the yyyy-mm-dd format. I.e. &lt;year&gt;-&lt;month as two digit number&gt;-&lt;day of month as two digit number&gt;</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="datetime-val" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation>For datetime attributes, contains the value of the attribute. Must be a valid date and time in the timezone for the policy model, in the format yyyy-mm-ddThh:mm:ss format. I.e. &lt;year&gt;-&lt;month as two digit number&gt;-&lt;day of month&gt;’T’&lt;hours 00-23&gt;:&lt;minutes 00-59&gt;:&lt;seconds 00-59&gt;</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="time-val" type="xsd:time">
                <xsd:annotation>
                    <xsd:documentation>For time attributes, contains the value of the attribute. Must be a valid time in hh:mm:ss format. I.e. &lt;hours 00-23&gt;:&lt;minutes 00-59&gt;:&lt;seconds 00-59&gt;</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="number-val" type="xsd:decimal">
                <xsd:annotation>
                    <xsd:documentation>For number or currency attributes, contains the value of the attribute</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="text-val" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>For text attributes, contains the value of the attribute </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="uncertain-val" type="UncertainValue">
                <xsd:annotation>
                    <xsd:documentation>For attributes of any type, this field is used when the attribute has the value uncertain</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="unknown-val" type="UnknownValue">
                <xsd:annotation>
                    <xsd:documentation>For attributes of any type, this field is used when the attribute has the value unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="date" type="xsd:date">
            <xsd:annotation>
                <xsd:documentation>date from which the new attribute value takes effect</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:simpleType name="RelationshipCardinalityEnum">
        <xsd:annotation>
            <xsd:documentation>The supported cardinality types for relationships provided to and returned from a Save data action.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="OneToOne">
                <xsd:annotation>
                    <xsd:documentation>Relationship cardinality is type one-to-one</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="OneToMany">
                <xsd:annotation>
                    <xsd:documentation>Relationship cardinality is type one-to-many</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ManyToOne">
                <xsd:annotation>
                    <xsd:documentation>Relationship cardinality is type many-to-one</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ManyToMany">
                <xsd:annotation>
                    <xsd:documentation>Relationship cardinality is type many-to-many</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>

            <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://oracle.com/determinations/server/interview/12.2.5/rulebase/types"
            xmlns:xds="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://oracle.com/determinations/server/interview/12.2.5/rulebase/types" elementFormDefault="qualified" version="1.0"
            xmlns:wsc="http://xmlns.oracle.com/connector/webservice/12.2.1/data/types">
    <xsd:import namespace="http://xmlns.oracle.com/connector/webservice/12.2.1/data/types" />

    <!-- ===========================  Requests =========================== -->

    <xsd:element name="start-interview-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="data" type="wsc:LoadData" minOccurs="0" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>This complex type contains data to seed into the interview. This is in the same format as the tables element of a Load response from a web service connector. The name of Fields and Links must match those returned by the GetInputDataDefinition response.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="show-version" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>True if version information about the policy model should be returned</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="end-interview-request">
        <xsd:complexType/>
    </xsd:element>

    <xsd:element name="investigate-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="goal-state" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>The goal-state of the currently displayed screen</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:choice minOccurs="0" maxOccurs="1">
                    <xsd:element name="screen" type="InterviewScreen">
                        <xsd:annotation>
                            <xsd:documentation>If an InterviewScreen is specified, this will include values for each of the controls that were previously returned by an Interview action.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="update-data" type="wsc:UpdateData">
                        <xsd:annotation>
                            <xsd:documentation>A set of data to send to the interview, including Field names, Link names and their targets. This is used on a screen who's ScreenAction is 'submit' following a previous Investigate response which included a submit-data node rather than a screen node. This is in the same format as an UpdateData type such as the tables element of a Save response from a web service connector.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:choice>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="snapshot-session-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="goal-state" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>The goal state identifier representing the current position in the interview. All the data collected so far is saved and upon a RestoreSession action the user will be placed back onto the screen corresponding to this goal state.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="restore-session-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="snapshot" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>A Base64 encoded string representing a session snapshot. This data has originally come from a SnapshotSession action.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="show-version" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>True if version information about the policy model should be returned</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="get-seed-data-definition-request">
        <xsd:complexType>
            <xsd:attribute name="show-version" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>True if version information about the policy model should be returned in the response</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="get-files-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:choice minOccurs="1" maxOccurs="unbounded">
                    <xsd:element name="attachment" type="RequestedAttachment">
                        <xsd:annotation>
                            <xsd:documentation>Request for a file that was attached via an attachment control during the interview</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="form" type="RequestedForm">
                        <xsd:annotation>
                            <xsd:documentation>Request for a form that was generated during the interview and has had its ID exposed via a Form control</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:choice>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="get-session-data-request">
        <xsd:complexType/>
    </xsd:element>

    <xsd:element name="set-session-data-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="global-instance" type="GlobalInstanceType" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>The global entity with attribute and relationship values to set in the session, along with child entities and their components.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <!-- ===========================  Responses =========================== -->
    <xsd:element name="start-interview-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="version-info" type="VersionInfoType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Version information about the policy model, if requested.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="goal-state" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>The goal state identifier for the screen currently being displayed.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="screen" type="InterviewScreen" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>A description of the visible, visited and current stages of the interview, as well as the labels and controls in the correct order for the current screen.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="end-interview-response">
        <xsd:complexType/>
    </xsd:element>

    <xsd:element name="investigate-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="goal-state" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>The goal-state of the currently displayed screen</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:choice minOccurs="0" maxOccurs="1">
                    <xsd:element name="submit-data" type="SubmitInfo">
                        <xsd:annotation>
                            <xsd:documentation>This is returned if the screen action of the request was 'submit' and included a screen node rather than an update-data node. It is a collection of data that is intended to be sent through a subsequent Investigate request.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="screen" type="InterviewScreen">
                        <xsd:annotation>
                            <xsd:documentation>A description of the visible, visited and current stages of the interview, as well as the labels and controls in the correct order for the current screen.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:choice>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="snapshot-session-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="snapshot" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>This is a Base64 encoded string representing the binary snapshot of the current interview state. This can be used in the RestoreSession action.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="restore-session-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="version-info" type="VersionInfoType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Version information about the policy model, if requested.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="warning-list" type="TransactionWarningList" minOccurs="0" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>A list of warnings associated with restoring the session</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="goal-state" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>The goal-state of the currently displayed screen</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="screen" type="InterviewScreen">
                    <xsd:annotation>
                        <xsd:documentation>A description of the visible, visited and current stages of the interview, as well as the labels and controls in the correct order for the current screen.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="get-files-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:choice minOccurs="1" maxOccurs="unbounded">
                    <xsd:element name="attachment" type="File">
                        <xsd:annotation>
                            <xsd:documentation>The requested attachment</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="form" type="File">
                        <xsd:annotation>
                            <xsd:documentation>The requested form</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:choice>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="get-seed-data-definition-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="version-info" type="VersionInfoType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Version information about the policy model, if requested.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="seed-data" type="SeedDataDefinition" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>A description of the mapped Tables which can be seeded, including their Field names, and Link names and targets. This is in the same format as the tables element of a Load response from a web service connector.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="SeedDataDefinition">
        <xsd:complexContent>
            <xsd:extension base="wsc:SeedDataDescription">
                <xsd:attribute name="root-table" use="required" type="xsd:string"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="get-session-data-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="global-instance" type="GlobalInstanceType" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>Data consisting of the attributes and relationships for the global entity instance, as well as contained entities and their attributes and relationships</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="set-session-data-response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="warnings" type="TransactionWarningList" minOccurs="0" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>A list of warnings associated with setting the session data</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!--error response-->
    <xsd:element name="error-response" type="Error"/>

    <xsd:complexType name="Error">
        <xsd:annotation>
            <xsd:documentation>When error event rules are triggered in a policy model during an interview action, a SOAP fault is generated containing the error details.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="code" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>The identifier of the error. Currently always "com.oracle.determinations.server.TransactionError"</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="message" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>The message. Currently always "The following error events were raised:"</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="transaction-errors" type="TransactionErrorList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Details of the error events that were raised during the interview action.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <!-- common types -->
    <xsd:simpleType name="AttributeTypeEnum">
        <xsd:annotation>
            <xsd:documentation>Enumeration description for a attribute-type attribute in a ValueExpressionBase, in AttributeExplanationNode, and in ExplanationControl representing an attribute data type in an Interview action.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="boolean">
                <xsd:annotation><xsd:documentation>Attribute is type Boolean</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="text">
                <xsd:annotation><xsd:documentation>Attribute is type Text</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="number">
                <xsd:annotation><xsd:documentation>Attribute is type Number</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="currency">
                <xsd:annotation><xsd:documentation>Attribute is type Currency</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="date">
                <xsd:annotation><xsd:documentation>Attribute is type Date</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="datetime">
                <xsd:annotation><xsd:documentation>Attribute is type Datetime</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="timeofday">
                <xsd:annotation><xsd:documentation>Attribute is type Timeofday</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="StateEnum">
        <xsd:annotation>
            <xsd:documentation>Enumeration description for a relationship-state attribute in a RelationshipExplanationNode representing a value's state in an Interview action.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="known">
                <xsd:annotation><xsd:documentation>The state of the object's value is known</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="unknown">
                <xsd:annotation><xsd:documentation>The state of the object's value is unknown</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="uncertain">
                <xsd:annotation><xsd:documentation>The state of the object's value is uncertain</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="UnknownValue">
        <xsd:annotation>
            <xsd:documentation>Used to indicate the unknown value state for attribute and relationship values provided to and returned from an Interview action.
                Tip: Any attributes, entities or relationships that are omitted from a request are also treated as unknown.</xsd:documentation>
        </xsd:annotation>
    </xsd:complexType>
    <xsd:complexType name="UncertainValue">
        <xsd:annotation>
            <xsd:documentation>Used to indicate the uncertain value for attribute values (and change points) provided to and returned from an Interview action. Unlike unknown, uncertain is considered a value, and prevents any further attempts to ask for or determine a value of that attribute.
                Uncertain is most often used when a question has been asked of a real person, and the response is given that no answer is available. For example, if the question is "have you been vaccinated against measles" and the answer is "I don't know" you might use the uncertain value to indicate that the individual has neither definitely been vaccinated nor definitely not been vaccinated against measles.</xsd:documentation>
        </xsd:annotation>
    </xsd:complexType>

    <xsd:simpleType name="RelationshipCardinalityEnum">
        <xsd:annotation>
            <xsd:documentation>Enumeration description for a relationship-type attribute in a RelationshipExplanationNode and ReferenceRelationshipControl representing the cardinality of a relationship.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="OneToOne">
                <xsd:annotation><xsd:documentation>The relationship's cardinality is one to one</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="OneToMany">
                <xsd:annotation><xsd:documentation>The relationship's cardinality is one to many</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ManyToOne">
                <xsd:annotation><xsd:documentation>The relationship's cardinality is many to one</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ManyToMany">
                <xsd:annotation><xsd:documentation>The relationship's cardinality is many to many</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ChangePoint">
        <xsd:annotation>
            <xsd:documentation>Temporal change points for attribute values and decision report nodes provided to and returned from an Interview action.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
            <xsd:element name="boolean-val" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>For boolean attributes, contains the value of the attribute. Must be set to either "true" or "false".</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="date-val" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation>For date attributes, contains the value of the attribute. Must be set to a valid date in the yyyy-mm-dd format. I.e. &lt;year&gt;-&lt;month as two digit number&gt;-&lt;day of month as two digit number&gt;</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="datetime-val" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation>For datetime attributes, contains the value of the attribute. Must be a valid date and time in the timezone for the policy model, in the format yyyy-mm-ddThh:mm:ss format. I.e. &lt;year&gt;-&lt;month as two digit number&gt;-&lt;day of month&gt;'T'&lt;hours 00-23&gt;:&lt;minutes 00-59&gt;:&lt;seconds 00-59&gt;</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="time-val" type="xsd:time">
                <xsd:annotation>
                    <xsd:documentation>For time attributes, contains the value of the attribute. Must be a valid time in hh:mm:ss format. I.e. &lt;hours 00-23&gt;:&lt;minutes 00-59&gt;:&lt;seconds 00-59&gt;</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="number-val" type="xsd:decimal">
                <xsd:annotation>
                    <xsd:documentation>For number or currency attributes, contains the value of the attribute</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="text-val" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>For text attributes, contains the value of the attribute </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="uncertain-val" type="UncertainValue">
                <xsd:annotation>
                    <xsd:documentation>For attributes of any type, this field is used when the attribute has the value uncertain</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="unknown-val" type="UnknownValue">
                <xsd:annotation>
                    <xsd:documentation>For attributes of any type, this field is used when the attribute has the value unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="date" type="xsd:date">
            <xsd:annotation>
                <xsd:documentation>The date from which the new attribute value takes effect</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- Errors that are related to an interview data transaction -->
    <xsd:complexType name="TransactionErrorList">
        <xsd:annotation>
            <xsd:documentation>Node for an error-list attribute in an InterviewScreen, and for many of the controls contained by an interview screen.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="error" type="TransactionMessage" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A translated message with an associated identifier for the issue</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="TransactionWarningList">
        <xsd:annotation>
            <xsd:documentation>Node for a warning-list attribute in an InterviewScreen, SetSessionData response, and for many of the controls contained by an interview screen.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="warning" type="TransactionMessage" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A translated message with an associated identifier for the issue</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="TransactionMessage">
        <xsd:annotation>
            <xsd:documentation>Node for an error attribute in a TransactionErrorList, and for warning in a TransactionWarningList. The string value contained by the TransactionMessage contains a translated message describing the issue, while the code is a short identifier of the issue.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>The localized message</xsd:documentation>
                </xsd:annotation>
                <xsd:attribute name="code" use="required" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>An identifier for the type of issue</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="SubmitInfo">
        <xsd:annotation>
            <xsd:documentation>Node for a submit-data attribute representing a set of data from an Investigate response.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="tables" type="wsc:SubmitData" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A set of data in the form of the web service connector's Save request</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="attachments" type="wsc:AttachmentList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A list of attachments generated or included by the interview</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="audit-reports" type="wsc:AuditReportList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A list of audit reports generated by the interview</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="root-table" use="required" type="xsd:string" />
    </xsd:complexType>

    <!-- get file types -->
    <xsd:complexType name="File">
        <xsd:annotation>
            <xsd:documentation>Node for a form or attachment attribute in a GetFiles response. The content of the node is a Base64 encoded string of the binary of the requested form or attachment.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="id" type="xsd:string" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The ID of the form or attachment that was requested</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="file-name" type="xsd:string" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The name of the form or attachment that was requested</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="RequestedForm">
        <xsd:annotation>
            <xsd:documentation>Node for an form attribute in a GetFiles request.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation><xsd:documentation>The ID of the form being requested</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="form-type" type="FormType" use="required">
            <xsd:annotation><xsd:documentation>The type of the form being requested</xsd:documentation></xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="RequestedAttachment">
        <xsd:annotation>
            <xsd:documentation>Node for an attachment attribute in a GetFiles request.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="id">
            <xsd:annotation><xsd:documentation>The ID of the attachment being requested</xsd:documentation></xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- Parent screen type -->
    <xsd:complexType name="InterviewScreen">
        <xsd:sequence>
            <xsd:element name="progress" type="InterviewProgress" minOccurs="0" maxOccurs="1" />
            <xsd:element name="properties" type="PropertiesList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A list of custom properties for the current screen</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="error-list" type="TransactionErrorList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A list of errors associated with this screen</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="warning-list" type="TransactionWarningList" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A list of warnings associated with this screen</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                    <xsd:element name="text-control" type="TextControl">
        <xsd:annotation><xsd:documentation>Collects text attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="date-control" type="DateControl">
        <xsd:annotation><xsd:documentation>Collects date attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="datetime-control" type="DateTimeControl">
        <xsd:annotation><xsd:documentation>Collects date time attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="currency-control" type="CurrencyControl">
        <xsd:annotation><xsd:documentation>Collects currency attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="boolean-control" type="BooleanControl">
        <xsd:annotation><xsd:documentation>Collects boolean attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="number-control" type="NumberControl">
        <xsd:annotation><xsd:documentation>Collects number attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="time-of-day-control" type="TimeOfDayControl">
        <xsd:annotation><xsd:documentation>Collects time of day attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="label-control" type="LabelControl">
        <xsd:annotation><xsd:documentation>A label control including any text substitutions</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="explanation-control" type="ExplanationControl">
        <xsd:annotation><xsd:documentation>An explanation control which described how the value of an attribute was attained</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="image-control" type="ImageControl">
        <xsd:annotation><xsd:documentation>Contains either a the URL of an image, or a Base64 encoded image</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="captcha-control" type="CaptchaControl">
        <xsd:annotation><xsd:documentation>A placeholder for a Captcha control, but implementation of validation is up to the client application</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="reference-relationship-control" type="ReferenceRelationshipControl">
        <xsd:annotation><xsd:documentation>A control which allows the user to set the targets of a given relationship</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="entity-group-control" type="EntityGroupControl">
        <xsd:annotation><xsd:documentation>This control represents a set of controls that are repeated for 0 or more instances of an entity</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="attachment-control" type="AttachmentControl">
        <xsd:annotation><xsd:documentation>Represents a control that can be used to attach files to the interview</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="signature-control" type="SignatureControl">
        <xsd:annotation><xsd:documentation>Represents a control that can be used to upload an image of a written signature to the interview</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="form-control" type="FormControl">
        <xsd:annotation><xsd:documentation>Represents a link to a generated Form</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="container-control" type="ContainerControl">
        <xsd:annotation><xsd:documentation>Represents a container of other controls, to be displayed out in a particular style</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="containment-relationship-control" type="ContainmentRelationshipControl">
        <xsd:annotation><xsd:documentation>A control that lets you create instances of the target entity of a containment relationship. It is also known as an entity collect control</xsd:documentation></xsd:annotation>
    </xsd:element>            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="id" use="required" type="xsd:string">
            <xsd:annotation><xsd:documentation>the id of the screen </xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="context-entity-id" use="required" type="xsd:string">
            <xsd:annotation><xsd:documentation>the name of the entity to which the screen belongs</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="context-instance-id" use="required" type="xsd:string">
            <xsd:annotation><xsd:documentation>the name of the entity instance to which the screen belongs</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="title" use="optional" type="xsd:string">
            <xsd:annotation><xsd:documentation>the screen's title, including text substitutions</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="is-automatic" use="optional" type="xsd:boolean">
            <xsd:annotation><xsd:documentation>If true, the screen has been automatically generated as opposed to being explicitly defined in the interview</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="screen-action" use="optional" type="ScreenAction">
            <xsd:annotation><xsd:documentation>The action associated with the screen. Can be one of the following:
                * submit - Indicates that when the data on this screen is successfully into the interview session, the application should be submitted to an external system
                * next - Indicates that when the data on this screen is successfully submitted into the session, the interview should be advanced to the next screen in the investigation
                * stop - This indicates that the interview should stop at this point</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="back-button-text" use="optional" type="xsd:string">
            <xsd:annotation><xsd:documentation>the text (if any) defined for the back button. It will not be present on the first visible screen in the interview. If this attribute is provided, the previous-goal-state will also be provided</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="submit-button-text" use="optional" type="xsd:string">
            <xsd:annotation><xsd:documentation>the text defined for the submit button. It will not be present on any screen with a screen-action of "stop". </xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="checkpoint-after-submit" use="optional" type="xsd:boolean">
            <xsd:annotation><xsd:documentation>indicates if the screen was selected by the Interview Designer to have a Checkpoint set after valid data is submitted (screen successfully submits without validation errors). A Checkpoint is a desired location within an interview where session data should be captured so the interview may later be resumed from that point. If the screen was a Submit screen, the Checkpoint should include load-after-submit data. The developer of the Interview Service is responsible saving the session state (the Checkpoint): the attribute should not be provided in a request to the Interview Service. </xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="previous-goal-state" use="optional" type="xsd:string">
            <xsd:annotation><xsd:documentation>a goal state representing first visible screen immediately prior this one in the interview. This will normally (but not always) be the previously displayed screen. </xsd:documentation></xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:simpleType name="ScreenAction">
        <xsd:annotation>
            <xsd:documentation>String enumeration for a screen-action attribute representing the submit-button action on an InterviewScreen.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="submit">
                <xsd:annotation>
                    <xsd:documentation>Indicates that when the data on this screen is successfully into the interview session, the application should be submitted to an external system</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="next">
                <xsd:annotation>
                    <xsd:documentation>Indicates that when the data on this screen is successfully submitted into the session, the interview should be advanced to the next screen in the investigation</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="stop">
                <xsd:annotation>
                    <xsd:documentation>This indicates that the interview should stop at this point</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <!--Progress Stages-->

    <xsd:complexType name="InterviewProgress">
        <xsd:sequence>
            <xsd:element name="progress-stage" type="ProgressStage" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A description of each stage of the interview, i.e. a title and whether it is the current stage, and screens within each stage</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="free-navigation" type="xsd:boolean" use="optional">
            <xsd:annotation><xsd:documentation>If true, indicates the interview was designed to navigate screens in any order.</xsd:documentation></xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="ProgressStage">
        <xsd:annotation>
            <xsd:documentation>Node for a progress-stage attribute under an anonymous complex type progress representing a stage in an InterviewScreen.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="progress-screen" type="ProgressScreen" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>0 or more screens in this stage</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="title" type="xsd:string" use="required">
            <xsd:annotation><xsd:documentation>The stage's title</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="is-current" type="xsd:boolean" use="required">
            <xsd:annotation><xsd:documentation>Indicates whether the current screen is part of this stage</xsd:documentation></xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="ProgressScreen">
        <xsd:annotation>
            <xsd:documentation>Node for a progress-screen attribute in a ProgressStage.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation><xsd:documentation>The screen's ID</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="goal-state" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>A goal state that can be used to jump to this screen</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="title" type="xsd:string" use="required">
            <xsd:annotation><xsd:documentation>The screen's title</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="will-show" type="xsd:boolean" use="required">
            <xsd:annotation>
                <xsd:documentation>Indicates that this is the screen currently being displayed</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="visited" type="xsd:boolean" use="required">
            <xsd:annotation>
                <xsd:documentation>Indicates whether this screen will be shown during the normal course of an investigation</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="all-relevant-data-collected" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="is-current" type="xsd:boolean" use="required">
            <xsd:annotation>
                <xsd:documentation>Indicates whether this screen has been visited during this investigation</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>


    <!-- Definitions common to all controls -->
    <xsd:complexType name="ControlBase">
        <xsd:annotation>
            <xsd:documentation>All interview controls extends this complex type, either directly or by extending InputControlBase.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="properties" type="PropertiesList" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="control-state-rule" type="ControlStateRule" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="id" use="optional" type="xsd:string"/>
        <xsd:attribute name="caption" use="optional" type="xsd:string"/>
        <xsd:attribute name="width" use="optional" type="xsd:integer"/>
        <xsd:attribute name="control-state" use="optional" type="ControlState"/>
        <xsd:attribute name="customRenderURL" use="optional" type="xsd:string"/>
    </xsd:complexType>

    <xsd:simpleType name="ControlState">
        <xsd:annotation>
            <xsd:documentation>String enumeration for a default-state attribute in a ControlStateRule.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="hidden"/>
            <xsd:enumeration value="read-only"/>
            <xsd:enumeration value="optional"/>
            <xsd:enumeration value="mandatory"/>
            <xsd:enumeration value="visible"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="ControlTypeEnum">
        <xsd:annotation>
            <xsd:documentation>String enumeration for a control-type attribute in a ReferenceRelationshipControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="checkbox">
                <xsd:annotation><xsd:documentation>represents a checkbox control</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="radio-button">
                <xsd:annotation><xsd:documentation>represents a radio button control</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="drop-down">
                <xsd:annotation><xsd:documentation>represents a drop down control</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="listbox">
                <xsd:annotation><xsd:documentation>represents a list box control</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="searching-combo">
                <xsd:annotation><xsd:documentation>represents a combobox where options are searchable</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="dmy-inputs">
                <xsd:annotation><xsd:documentation>represents a coded input field for dates where there are separate inputs for the day, month and year values </xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="calendar">
                <xsd:annotation><xsd:documentation>represents a control that allows date values to be selected from a calendar</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="text-area">
                <xsd:annotation><xsd:documentation>represents a multi-line text input control</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="text">
                <xsd:annotation><xsd:documentation>represents a single line text input control</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="masked"/>
            <xsd:enumeration value="password"/>
            <xsd:enumeration value="hms-inputs"/>
            <xsd:enumeration value="dmyhms-inputs"/>
            <xsd:enumeration value="image-button"/>
            <xsd:enumeration value="text-button-group"/>
            <xsd:enumeration value="image-button-group"/>
            <xsd:enumeration value="text-image-button-group"/>
            <xsd:enumeration value="slider"/>
            <xsd:enumeration value="switch"/>
            <xsd:enumeration value="custom">
                <xsd:annotation><xsd:documentation>a custom input control</xsd:documentation></xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="EntityContainerDisplayTypeEnum">
        <xsd:annotation><xsd:documentation>The display style of the entity control</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="portrait">
                <xsd:annotation>
                    <xsd:documentation>each child instance should be rendered vertically, in a business card fashion down the page</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="tabular">
                <xsd:annotation>
                    <xsd:documentation>the control should be rendered as a table, with the controls representing the columns and the instances the rows</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
