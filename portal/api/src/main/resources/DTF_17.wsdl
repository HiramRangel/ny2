    <!--Explanation Control -->
    <xsd:complexType name="ExplanationControl">
        <xsd:annotation>
            <xsd:documentation>Node for a explanation-control attribute representing an explanation control in an InterviewScreen, EntityInstanceControl and ContainerControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:choice minOccurs="0">
                        <xsd:element name="boolean-val" type="xsd:boolean"/>
                        <xsd:element name="date-val" type="xsd:date"/>
                        <xsd:element name="datetime-val" type="xsd:dateTime"/>
                        <xsd:element name="time-val" type="xsd:time"/>
                        <xsd:element name="number-val" type="xsd:decimal"/>
                        <xsd:element name="text-val" type="xsd:string"/>
                        <xsd:element name="uncertain-val" type="UncertainValue"/>
                        <xsd:element name="unknown-val" type="UnknownValue"/>
                    </xsd:choice>
                    <xsd:element name="change-point" type="ChangePoint" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                        <xsd:element name="attribute-node" type="AttributeExplanationNode"/>
                        <xsd:element name="relationship-node" type="RelationshipExplanationNode"/>
                    </xsd:choice>
                </xsd:sequence>
                <xsd:attribute name="attribute-id" type="xsd:string" use="required"/>
                <xsd:attribute name="attribute-type" type="AttributeTypeEnum" use="required"/>
                <xsd:attribute name="context-entity-id" type="xsd:string" use="required"/>
                <xsd:attribute name="context-instance-id" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="AttributeExplanationNode">
        <xsd:annotation>
            <xsd:documentation>Node for a attribute-node attribute representing a node within an ExplanationControl in an InterviewScreen.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <!--node attribute value-->
                    <xsd:choice minOccurs="0">
                        <xsd:element name="boolean-val" type="xsd:boolean"/>
                        <xsd:element name="date-val" type="xsd:date"/>
                        <xsd:element name="datetime-val" type="xsd:dateTime"/>
                        <xsd:element name="time-val" type="xsd:time"/>
                        <xsd:element name="number-val" type="xsd:decimal"/>
                        <xsd:element name="text-val" type="xsd:string"/>
                        <xsd:element name="uncertain-val" type="UncertainValue"/>
                        <xsd:element name="unknown-val" type="UnknownValue"/>
                    </xsd:choice>
                    <xsd:element name="change-point" type="ChangePoint" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                        <xsd:element name="attribute-node" type="AttributeExplanationNode"/>
                        <xsd:element name="relationship-node" type="RelationshipExplanationNode"/>
                        <xsd:element name="already-proven-node" type="ReferenceExplanationNode"/>
                    </xsd:choice>
                    <xsd:element name="rule-tags" type="RuleTagList" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Contains the rule tags for this explanation node.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="attribute-id" type="xsd:string" use="required"/>
                <xsd:attribute name="attribute-type" type="AttributeTypeEnum" use="required"/>
                <xsd:attribute name="context-entity-id" type="xsd:string" use="required"/>
                <xsd:attribute name="context-instance-id" type="xsd:string" use="required"/>
                <xsd:attribute name="is-user-set" type="xsd:boolean" use="required"/>
                <xsd:attribute name="is-base-level" type="xsd:boolean" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="RelationshipExplanationNode">
        <xsd:annotation>
            <xsd:documentation>Node for a relationship-node attribute representing a node within an ExplanationControl in an InterviewScreen.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                        <xsd:element name="entity-node" type="EntityExplanationNode"/>
                        <xsd:element name="attribute-node" type="AttributeExplanationNode"/>
                        <xsd:element name="relationship-node" type="RelationshipExplanationNode"/>
                        <xsd:element name="already-proven-node" type="ReferenceExplanationNode"/>
                    </xsd:choice>
                    <xsd:element name="rule-tags" type="RuleTagList" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Contains the rule tags for this explanation node.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="relationship-id" type="xsd:string" use="required"/>
                <xsd:attribute name="relationship-type" type="RelationshipCardinalityEnum" use="required"/>
                <xsd:attribute name="source-entity-id" type="xsd:string" use="required"/>
                <xsd:attribute name="source-instance-id" type="xsd:string" use="required"/>
                <xsd:attribute name="target-entity-id" type="xsd:string" use="required"/>
                <xsd:attribute name="relationship-state" type="StateEnum" use="required"/>
                <xsd:attribute name="is-user-set" type="xsd:boolean" use="required"/>
                <xsd:attribute name="is-base-level" type="xsd:boolean" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="EntityExplanationNode">
        <xsd:annotation>
            <xsd:documentation>Node for a entity-node attribute in RelationshipExplanationNode within an ExplanationControl in an InterviewScreen.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:attribute name="instance-id" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ReferenceExplanationNode">
        <xsd:annotation>
            <xsd:documentation>Node for an already-proven-node attribute in a RelationshipExplanationNode and an AttributeExplanationNode within an ExplanationControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="id" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="RuleTagList">
        <xsd:annotation>
            <xsd:documentation>A container for rule tags</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="rule-tag" type="RuleTag" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The rule tag definition</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RuleTag">
        <xsd:annotation>
            <xsd:documentation>The rule tag</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <!-- image control -->
    <xsd:complexType name="ImageControl">
        <xsd:annotation>
            <xsd:documentation>Node for a image-control attribute representing an image control in an InterviewScreen, EntityInstanceControl and ContainerControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:element name="image" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>A Base64 encoded string of the image, if a URL is not provided</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="url" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The URL of the image, if a Base64 encoded string is not provided in image</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="height" type="xsd:integer" use="optional">
                    <xsd:annotation><xsd:documentation>The height of the image</xsd:documentation></xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="horizontal-align" type="HorizontalAlignmentEnum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The horizontal alignment of the image. Can be one of:
                            * left
                            * right
                            * center</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!-- CAPTCHA Control -->
    <xsd:complexType name="CaptchaControl">
        <xsd:annotation>
            <xsd:documentation>Node for a captcha-control attribute representing a Captcha control in an InterviewScreen, EntityInstanceControl and ContainerControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase"/>
        </xsd:complexContent>
    </xsd:complexType>


    <!-- Form Control -->
    <xsd:simpleType name="FormType">
        <xsd:annotation>
            <xsd:documentation>String enumeration for a form-type attribute representing the type of form which a FormControl or RequestedForm represents.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Excel">
                <xsd:annotation>
                    <xsd:documentation>signifies a Microsoft Excel document should be generated</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="HTML">
                <xsd:annotation>
                    <xsd:documentation>signifies a Hypertext Markup Language document should be generated</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PDF">
                <xsd:annotation>
                    <xsd:documentation>signifies a Portable Document Format document should be generated</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="RTF">
                <xsd:annotation>
                    <xsd:documentation>signifies an Rich Text Format document should be generated</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="XML">
                <xsd:annotation>
                    <xsd:documentation>signifies an Extensible Markup Language document should be generated</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="FormControl">
        <xsd:annotation><xsd:documentation></xsd:documentation></xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:attribute name="form-id" type="xsd:string" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The ID the form this control links to. The document itself can be generated via the Get Files action</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="form-type" type="FormType" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The type of document to generate. Can be one of:
                            * Excel - signifies a Microsoft Excel document should be generated
                            * HTML - signifies a Hypertext Markup Language document should be generated
                            * PDF - signifies a Portable Document Format document should be generated
                            * RTF - signifies an Rich Text Format document should be generated
                            * XML - signifies a Extensible Markup Language document should be generate</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!-- AttachmentControl -->
    <xsd:complexType name="AttachmentControl">
        <xsd:annotation>
            <xsd:documentation>Node for a attachment-control attribute representing a control in an InterviewScreen, EntityInstanceControl and ContainerControl</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:element name="error-list" type="TransactionErrorList" minOccurs="0" maxOccurs="1">
                        <xsd:annotation><xsd:documentation>List of errors associated with this control</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="warning-list" type="TransactionWarningList" minOccurs="0" maxOccurs="1">
                        <xsd:annotation><xsd:documentation>List of warnings associated with this control</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="files-to-upload" type="NewAttachmentList" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>A list of files to upload. To upload a file the client should create one or more file elements to this element. Each file element must have an attribute file-name which specifies the name of the file, whilst the element should contain the file's content as a Base64 encoded string. NOTE: To cut down on the amount of data being transmitted, if a screen submission fails the set of files to upload provided in the request will not be reflected back in the response. The client will need to resupply the files to upload on the next request</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="existing-files" type="SessionAttachmentList" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>The set of all files currently attached to the session. This element will contain one or more file elements which will specify the id and the name of the attached file via the attachment-id and file-name parameters respectively. The file id can then be used to retrieve the attachment's contents via the Get Files action.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!-- SignatureControl -->
    <xsd:complexType name="SignatureControl">
        <xsd:annotation>
            <xsd:documentation>Node for a signature-control attribute representing a control in an InterviewScreen, EntityInstanceControl and ContainerControl</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ControlBase">
                <xsd:sequence>
                    <xsd:element name="error-list" type="TransactionErrorList" minOccurs="0" maxOccurs="1">
                        <xsd:annotation><xsd:documentation>List of errors associated with this control</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="warning-list" type="TransactionWarningList" minOccurs="0" maxOccurs="1">
                        <xsd:annotation><xsd:documentation>List of warnings associated with this control</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="file-to-upload" type="NewAttachment" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>The file to upload. To upload a file the client should create one or more file elements to this element. Each file element must have an attribute file-name which specifies the name of the file, whilst the element should contain the file's content as a Base64 encoded string. NOTE: To cut down on the amount of data being transmitted, if a screen submission fails the set of files to upload provided in the request will not be reflected back in the response. The client will need to resupply the files to upload on the next request</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="existing-file" type="SessionAttachment" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>The set of all files currently attached to the session. This element will contain one or more file elements which will specify the id and the name of the attached file via the attachment-id and file-name parameters respectively. The file id can then be used to retrieve the attachment's contents via the Get Files action.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="SessionAttachmentList">
        <xsd:annotation>
            <xsd:documentation>Node for a existing-files attribute representing a list of attachments in an AttachmentControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="file" type="SessionAttachment" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation><xsd:documentation>An attachment</xsd:documentation></xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SessionAttachment">
        <xsd:annotation>
            <xsd:documentation>Node for a file attribute representing an attachment in an SessionAttachmentList.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="attachment-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The stored ID which can be used to retrieve the file via the Get Files action</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="file-name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The file name of the attachment</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="NewAttachmentList">
        <xsd:annotation>
            <xsd:documentation>Node for a files-to-upload attribute representing a list of attachments in an AttachmentControl.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="file" type="NewAttachment" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>An attachment</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
