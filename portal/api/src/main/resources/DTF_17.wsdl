        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>The body of the attachment as a Base64 encoded string</xsd:documentation>
                </xsd:annotation>
                <xsd:attribute name="file-name" type="xsd:string" use="required">
                    <xsd:annotation>
                        <xsd:documentation>The file name of the attachment</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <!-- Custom Properties -->
    <xsd:complexType name="PropertiesList">
        <xsd:annotation>
            <xsd:documentation>Node for a properties attribute in an InterviewScreen, and for many of the controls contained by an interview screen.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="property" type="PropertiesListItem" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation><xsd:documentation>A property in the list of properties</xsd:documentation></xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PropertiesListItem">
        <xsd:annotation>
            <xsd:documentation>Node for a property attribute in a PropertiesList. The string value contained by the PropertiesListItem describes the value for the type of property represented, which is indicated by the value of the key attribute</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:annotation><xsd:documentation>the value for the type of property represented</xsd:documentation></xsd:annotation>
                <xsd:attribute name="key" use="required" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>An identifier for the type of property which the PropertiesListItem represents</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <!-- Dynamic Control State -->
    <xsd:simpleType name="ExpressionOperator">
        <xsd:annotation>
            <xsd:documentation>String enumeration for an expression-operator attribute in a ValueExpressionBase.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="equals"/>
            <xsd:enumeration value="not-equals"/>
            <xsd:enumeration value="in"/>
            <xsd:enumeration value="not-in"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="ExpressionType">
        <xsd:annotation>
            <xsd:documentation>String enumeration for an expression-type attribute in a ValueExpressionBase.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="if"/>
            <xsd:enumeration value="unless"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ControlStateRule">
        <xsd:annotation>
            <xsd:documentation>Node for a control-state-rule attribute in node extending ControlBase.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="hide" type="ControlStateExpression" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="read-only" type="ControlStateExpression" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="optional" type="ControlStateExpression" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="default-state" type="ControlState" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="ControlStateExpression">
        <xsd:annotation>
            <xsd:documentation>Node for hide, read-only, and option attributes in a ControlStateRule.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:choice maxOccurs="1" minOccurs="1">
                <xsd:element name="constant-expression" type="ConstantExpression"/>
                <xsd:element name="number-expression" type="NumberValueExpression"/>
                <xsd:element name="text-expression" type="TextValueExpression"/>
                <xsd:element name="boolean-expression" type="BooleanValueExpression"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ConstantExpression">
        <xsd:annotation>
            <xsd:documentation>Node for a constant-expression attribute in a ControlStateExpression.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="result" type="xsd:boolean"/>
    </xsd:complexType>

    <xsd:complexType name="ValueExpressionBase">
        <xsd:annotation>
            <xsd:documentation>Complex type definition to be extended by other value expressions.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="expression-type" type="ExpressionType" use="required"/>
        <xsd:attribute name="control-id" type="xsd:string" use="required"/>
        <xsd:attribute name="attribute-id" type="xsd:string" use="required"/>
        <xsd:attribute name="attribute-type" type="AttributeTypeEnum" use="required"/>
        <xsd:attribute name="entity-id" type="xsd:string" use="required"/>
        <xsd:attribute name="instance-id" type="xsd:string" use="required"/>
        <xsd:attribute name="expression-operator" type="ExpressionOperator" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="NumberValueExpression">
        <xsd:annotation>
            <xsd:documentation>Node for a number-expression attribute in a ControlStateExpression.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ValueExpressionBase">
                <xsd:sequence>
                    <xsd:choice minOccurs="1" maxOccurs="unbounded">
                        <xsd:element name="uncertain-val" type="UncertainValue"/>
                        <xsd:element name="unknown-val" type="UnknownValue"/>
                        <xsd:element name="value" type="xsd:decimal"/>
                    </xsd:choice>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="TextValueExpression">
        <xsd:annotation>
            <xsd:documentation>Node for a text-expression attribute in a ControlStateExpression.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ValueExpressionBase">
                <xsd:sequence>
                    <xsd:choice minOccurs="1" maxOccurs="unbounded">
                        <xsd:element name="uncertain-val" type="UncertainValue"/>
                        <xsd:element name="unknown-val" type="UnknownValue"/>
                        <xsd:element name="value" type="xsd:string"/>
                    </xsd:choice>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="BooleanValueExpression">
        <xsd:annotation>
            <xsd:documentation>Node for a boolean-expression attribute in a ControlStateExpression.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ValueExpressionBase">
                <xsd:sequence>
                    <xsd:choice minOccurs="1" maxOccurs="unbounded">
                        <xsd:element name="uncertain-val" type="UncertainValue"/>
                        <xsd:element name="unknown-val" type="UnknownValue"/>
                        <xsd:element name="value" type="xsd:boolean"/>
                    </xsd:choice>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!--Session data types -->
    <xsd:complexType name="GlobalInstanceType">
        <xsd:annotation>
            <xsd:documentation>Data provided to and returned from a SetSessionData or GetSessionData action, consisting of the attributes and relationships for the global entity instance, as well as contained entities and their attributes and relationships.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attribute" type="AttributeType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Attributes of the one global entity instance</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="relationship" type="RelationshipType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Non-containment relationships from the global entity instance to other entities </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="entity" type="EntityType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Child entities contained by the global entity instance</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="EntityType">
        <xsd:sequence>
            <xsd:element name="instance" type="EntityInstanceType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>All entity instances for this entity</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation><xsd:documentation>The name of the entity</xsd:documentation></xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="EntityInstanceType">
        <xsd:annotation>
            <xsd:documentation>Entity instance data provided to and returned from an Interview action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attribute" type="AttributeType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Attributes for the entity instance</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="relationship" type="RelationshipType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Relationships from this entity instance to other entities</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="entity" type="EntityType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Other entities contained by this entity instance</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>A unique identifier for the entity instance</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="RelationshipType">
        <xsd:annotation>
            <xsd:documentation>Information about entity reference relationships provided to and returned from an Interview action.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="target" type="RelationshipTargetType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The target entity instances for the relationship</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The name of the relationship </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="state" type="StateEnum">
            <xsd:annotation>
                <xsd:documentation>The outcome state of the relationship</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="RelationshipTargetType">
        <!--the value of this attribute must match the id one of the specified entity instances -->
        <xsd:annotation>
            <xsd:documentation>Target entity instances of a relationship provided to and returned from an Interview action.
                This is a reference to the target entity instance, being an XSD:ID (an XSD:ID is an Extensible Markup Language (XML) Schema Definition Identifier). It must be unique and must match an entities id attribute (see Node:entity).</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="instance-id" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>The identifier of a target entity instance of the relationship. This much match an entity instance's id attribute.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="AttributeType">
        <xsd:annotation>
            <xsd:documentation>Attribute values and metadata provided to and returned from an Interview action.
                Attribute nodes are used either to:
                1. Set an attribute's input value
                2. Indicate the level of outcome detail to include in the response for an attribute
                A value must be provided if no outcome-style is specified, and that value will be used (and returned in the response) even if the attribute is able to be inferred by the policy model. If an outcome-style is specified, any value provided for the attribute is ignored.
                Where an attribute is linked to a value list, ensure that any value you set for the attribute is a value on the value list. Otherwise, the web service will return an error: "The value for attribute &lt;attribute&gt; should be an enumerated value".
                On input, for attributes with an unknown value, either leave the attribute out of the request entirely, or use the unknown-val element.
                Notes:
                * Do not attempt to simultaneously set a value and specify an outcome style for the same attribute.
                * Where an attribute is linked to a value list, and you attempt to set a value for that attribute which is not in the value list, the web service will return an error: "The value for attribute &lt;attribute&gt; should be an enumerated value".
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <!--node attribute value-->
            <xsd:choice minOccurs="0">
                <xsd:element name="boolean-val" type="xsd:boolean">
                    <xsd:annotation><xsd:documentation>For boolean attributes, contains the value of the attribute. Must be set to either "true" or "false".</xsd:documentation></xsd:annotation>
                </xsd:element>
                <xsd:element name="date-val" type="xsd:date">
                    <xsd:annotation><xsd:documentation>For date attributes, contains the value of the attribute. Must be set to a valid date in the yyyy-mm-dd format. I.e. &lt;year&gt;-&lt;month as two digit number&gt;-&lt;day of month as two digit number&gt;</xsd:documentation></xsd:annotation>
                </xsd:element>
                <xsd:element name="datetime-val" type="xsd:dateTime">
                    <xsd:annotation><xsd:documentation>For datetime attributes, contains the value of the attribute. Must be a valid date and time in the timezone for the policy model, in the format yyyy-mm-ddThh:mm:ss format. I.e. &lt;year&gt;-&lt;month as two digit number&gt;-&lt;day of month&gt;'T'&lt;hours 00-23&gt;:&lt;minutes 00-59&gt;:&lt;seconds 00-59&gt;</xsd:documentation></xsd:annotation>
                </xsd:element>
                <xsd:element name="time-val" type="xsd:time">
                    <xsd:annotation><xsd:documentation>For time attributes, contains the value of the attribute. Must be a valid time in hh:mm:ss format. I.e. &lt;hours 00-23&gt;:&lt;minutes 00-59&gt;:&lt;seconds 00-59&gt;</xsd:documentation></xsd:annotation>
                </xsd:element>
                <xsd:element name="number-val" type="xsd:decimal">
                    <xsd:annotation><xsd:documentation>For number or currency attributes, contains the value of the attribute</xsd:documentation></xsd:annotation>
                </xsd:element>
                <xsd:element name="text-val" type="xsd:string">
                    <xsd:annotation><xsd:documentation>For text attributes, contains the value of the attribute</xsd:documentation></xsd:annotation>
                </xsd:element>
                <xsd:element name="uncertain-val" type="UncertainValue">
                    <xsd:annotation><xsd:documentation>For attributes of any type, this field is used when the attribute has the value uncertain</xsd:documentation></xsd:annotation>
                </xsd:element>
                <xsd:element name="unknown-val" type="UnknownValue">
                    <xsd:annotation><xsd:documentation>For attributes of any type, this field is used when the attribute has the value unknown</xsd:documentation></xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="change-point" type="ChangePoint" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>When an attribute has temporal change points, this sequence of fields indicate where those change points occur</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation><xsd:documentation>The name of the attribute</xsd:documentation></xsd:annotation>
        </xsd:attribute>
        <!--this property is returned for each attribute in a response -->
        <xsd:attribute name="type" type="AttributeTypeEnum" use="optional">
            <xsd:annotation><xsd:documentation>Ignored in requests. The data type of the attribute.</xsd:documentation></xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- XSD types for version information -->
<xsd:complexType name="VersionInfoType">
    <xsd:annotation>
        <xsd:documentation>The version information of a deployed policy model</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
        <xsd:element name="deployment-version" type="xsd:int" minOccurs="0">
