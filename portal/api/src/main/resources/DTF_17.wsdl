            <xsd:annotation>
                <xsd:documentation>The unique identifier, tagged at compile time by Policy Modeling</xsd:documentation>
            </xsd:annotation>
        </xsd:element>
        <xsd:element name="build-time" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
                <xsd:documentation>The date and time the policy model was built, automatically tagged by Policy Modeling</xsd:documentation>
            </xsd:annotation>
        </xsd:element>
        <xsd:element name="policy-modeling-version" type="xsd:string" minOccurs="0">
            <xsd:annotation>
                <xsd:documentation>The user-defined version identifier, entered in the Policy Modeling interface</xsd:documentation>
            </xsd:annotation>
        </xsd:element>
    </xsd:sequence>
</xsd:complexType>
</xsd:schema>

