    <xsd:element name="date-control" type="DateControl">
        <xsd:annotation><xsd:documentation>Collects date attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="datetime-control" type="DateTimeControl">
        <xsd:annotation><xsd:documentation>Collects date time attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="currency-control" type="CurrencyControl">
        <xsd:annotation><xsd:documentation>Collects currency attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="boolean-control" type="BooleanControl">
        <xsd:annotation><xsd:documentation>Collects boolean attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="number-control" type="NumberControl">
        <xsd:annotation><xsd:documentation>Collects number attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="time-of-day-control" type="TimeOfDayControl">
        <xsd:annotation><xsd:documentation>Collects time of day attribute</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="label-control" type="LabelControl">
        <xsd:annotation><xsd:documentation>A label control including any text substitutions</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="explanation-control" type="ExplanationControl">
        <xsd:annotation><xsd:documentation>An explanation control which described how the value of an attribute was attained</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="image-control" type="ImageControl">
        <xsd:annotation><xsd:documentation>Contains either a the URL of an image, or a Base64 encoded image</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="captcha-control" type="CaptchaControl">
        <xsd:annotation><xsd:documentation>A placeholder for a Captcha control, but implementation of validation is up to the client application</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="reference-relationship-control" type="ReferenceRelationshipControl">
        <xsd:annotation><xsd:documentation>A control which allows the user to set the targets of a given relationship</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="entity-group-control" type="EntityGroupControl">
        <xsd:annotation><xsd:documentation>This control represents a set of controls that are repeated for 0 or more instances of an entity</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="attachment-control" type="AttachmentControl">
        <xsd:annotation><xsd:documentation>Represents a control that can be used to attach files to the interview</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="signature-control" type="SignatureControl">
        <xsd:annotation><xsd:documentation>Represents a control that can be used to upload an image of a written signature to the interview</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="form-control" type="FormControl">
        <xsd:annotation><xsd:documentation>Represents a link to a generated Form</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="container-control" type="ContainerControl">
        <xsd:annotation><xsd:documentation>Represents a container of other controls, to be displayed out in a particular style</xsd:documentation></xsd:annotation>
    </xsd:element>
    <xsd:element name="containment-relationship-control" type="ContainmentRelationshipControl">
        <xsd:annotation><xsd:documentation>A control that lets you create instances of the target entity of a containment relationship. It is also known as an entity collect control</xsd:documentation></xsd:annotation>
    </xsd:element>                    </xsd:choice>
                </xsd:sequence>
                <xsd:attribute name="display-style" type="ContainerDisplayTypeEnum" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>The display style of the control. Can be:
                            * horizontal - the control should be rendered with each of its child controls laid out in a single row across the page
                            * vertical - the control should be rendered with each of its child controls laid out vertically, one control per row going down the page.</xsd:documentation>
